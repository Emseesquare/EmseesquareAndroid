<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/home_nav_graph"
    app:startDestination="@+id/homeFragment">

    <action
        android:id="@+id/toAddNotice"
        app:destination="@+id/addNotice"
        app:enterAnim="@anim/slide_in_from_right"
        app:exitAnim="@anim/slide_out_to_left"
        app:popEnterAnim="@anim/slide_in_from_left"
        app:popExitAnim="@anim/slide_out_to_right" />
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.fourteen06.emseesquare.presentation.notice.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/addNotice"
        android:name="com.fourteen06.emseesquare.presentation.add_notice.AddNotice"
        android:label="AddNotice" />

</navigation>