<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/authFragment2">


    <fragment
        android:id="@+id/authFragment2"
        android:name="com.fourteen06.emseesquare.presentation.auth.AuthFragment"
        android:label="fragment_login_with_phone_number"
        tools:layout="@layout/fragment_login_with_phone_number" >
        <action
            android:id="@+id/action_authFragment2_to_otpVerifyFragment"
            app:destination="@id/otpVerifyFragment" />
    </fragment>
    <fragment
        android:id="@+id/otpVerifyFragment"
        android:name="com.fourteen06.emseesquare.presentation.auth.OtpVerifyFragment"
        android:label="OtpVerifyFragment" >
        <argument
            android:name="args"
            app:argType="com.fourteen06.emseesquare.models.AuthNavArgs" />
    </fragment>
</navigation>